{
  "name": "genome-cli",
  "version": "0.0.1",
  "description": "CLI tool to help analyze ones raw sequenced DNA",
  "main": "index.js",
  "scripts": {
    "build": "babel source --presets babel-preset-es2015 --out-dir distribution",
    "dev": "babel source --presets babel-preset-es2015 --out-dir distribution && node ./distribution/index.js",
    "lint": "standard *.js source/*.js tests/*.js",
    "prepublish": "npm run build",
    "start": "echo STARTING GENOME-CLI... && babel source --presets babel-preset-es2015 --out-dir distribution --quiet && node ./distribution/index.js",
    "test": "npm run build && tap ./tests/*.js"
  },
  "keywords": [
    "cli",
    "CLI",
    "genome",
    "genoset",
    "genome.js",
    "genosets",
    "DNA"
  ],
  "author": "Kevin Collins <kevincol54@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "6.0.0",
    "npm": "3.8.6"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "dna2json": "^1.0.1",
    "genoset-101": "0.0.2",
    "genoset-119": "^1.0.1",
    "genoset-122": "^1.0.0",
    "genoset-137": "^1.0.0",
    "genoset-150": "^1.0.1",
    "genoset-152": "^1.0.1",
    "genoset-159": "^1.0.4",
    "genoset-209": "^1.0.0",
    "genoset-243": "^1.0.1",
    "genoset-248": "^1.0.1",
    "genoset-male": "^1.0.0",
    "image-to-ascii": "^3.0.2",
    "inquirer": "^0.12.0",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "rewire": "^2.5.1",
    "standard": "^6.0.5"
  }
}
